# API Configuration - Change based on environment
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_API_VERSION=v1
REACT_APP_ENVIRONMENT=development

# Application Configuration
REACT_APP_APP_NAME=PestControl CRM
REACT_APP_VERSION=1.0.0

# Debug and Development
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# API Configuration
REACT_APP_HEALTH_CHECK_INTERVAL=30000
REACT_APP_REQUEST_TIMEOUT=10000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# For Production (Railway), use:
# REACT_APP_API_BASE_URL=https://pestcontrol-backend-production.up.railway.app/api
# REACT_APP_ENVIRONMENT=production
# REACT_APP_DEBUG_MODE=false
# REACT_APP_LOG_LEVEL=error